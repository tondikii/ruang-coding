<%- include('partials/header', {title: 'My Course'}) %>
<%- include('partials/navbar') %>

<div class="row row-cols-1 row-cols-md-4 g-4">
    <% data.forEach(el => { %>
      <div class="col">
      <div class="card" style="width: 18rem">
          <img src="<%= el.dataValues.imageUrl %>" class="card-img-top" alt="" />
          <div class="card-body">
            <h5 class="card-title"><%= el.dataValues.name %></h5>
            <p class="card-text">price: <%= currency(el.dataValues.price) %></p>
            <div class="button">
              <a href="/ruangcoding/home/<%= el.dataValues.id %>" class="btn btn-warning">Buy</a>
            </div>
          </div>
        </div>
    </div>
    <% }) %>
  </div>

<%- include('partials/footer') %>